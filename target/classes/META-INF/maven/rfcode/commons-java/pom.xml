<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>rfcode</groupId>
	<artifactId>commons-java</artifactId>
	<version>0.0.1</version>
	<name>commons-java</name>
	<description>Utilities and Commons to java development</description>
	<licenses>
		<license>
			<name>The Apache License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
		</license>
	</licenses>
	<developers>
		<developer>
			<name>Reginaldo Ferreira Costa Junior</name>
			<email>regcostajr@gmail.com</email>
			<organization>Crypto</organization>
			<organizationUrl>cryptorocks.com</organizationUrl>
		</developer>
	</developers>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<resources>
			<resource>
				<directory>src</directory>
				<includes>
					<include>**</include>
				</includes>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
		<extensions>
			<extension>
				<groupId>org.springframework.build</groupId>
				<artifactId>aws-maven</artifactId>
				<version>5.0.0.RELEASE</version>
			</extension>
		</extensions>
	</build>
	<!--<distributionManagement>
		<snapshotRepository>
			<id>Commons.Java-snapshot</id>
			<url>s3://repository.rfcode.com.br/commons-java/snapshot</url>
		</snapshotRepository>
		<repository>
			<id>Commons.Java-release</id>
			<url>s3://repository.rfcode.com.br/commons-java/release</url>
		</repository>
	</distributionManagement>-->
	<properties>
		<commons-lang3>3.0</commons-lang3>
		<commons-net>3.5</commons-net>
		<commons-io>2.5</commons-io>
		<commons-dbutils>1.6</commons-dbutils>
		<commons-email>1.3.2</commons-email>
		<commons-codec>1.4</commons-codec>
		<jcraft>0.1.53</jcraft>
		<icu4j>57.1</icu4j>
		<poi>3.14</poi>
		<pdfbox>2.0.0-RC2</pdfbox>
		<jets3t>0.9.4</jets3t>
		<junit>4.12</junit>
		<xstream>1.4.9</xstream>
		<jettison>1.3.7</jettison>
		<google-maps>0.1.15</google-maps>
		<hikari>2.4.5</hikari>
		<jtds>1.3.1</jtds>
		<mysql>5.1.38</mysql>
		<slf4j>1.7.21</slf4j>
		<tess4j>3.2.1</tess4j>
		<hibernate>5.2.2.Final</hibernate>
	</properties>
	<dependencies>
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
			<version>${commons-lang3}</version>
		</dependency>
		<dependency>
			<groupId>commons-net</groupId>
			<artifactId>commons-net</artifactId>
			<version>${commons-net}</version>
		</dependency>
		<dependency>
			<groupId>commons-io</groupId>
			<artifactId>commons-io</artifactId>
			<version>${commons-io}</version>
		</dependency>
		<dependency>
			<groupId>commons-dbutils</groupId>
			<artifactId>commons-dbutils</artifactId>
			<version>${commons-dbutils}</version>
		</dependency>
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-email</artifactId>
			<version>${commons-email}</version>
		</dependency>
		<dependency>
			<groupId>com.jcraft</groupId>
			<artifactId>jsch</artifactId>
			<version>${jcraft}</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.icu</groupId>
			<artifactId>icu4j</artifactId>
			<version>${icu4j}</version>
		</dependency>
		<dependency>
			<groupId>org.apache.poi</groupId>
